{
  "name": "rscript-not-found",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --asar --platform=darwin --arch=x64 --extra-resource=./r_codes --prune=true --out=release-builds-mac",
    "package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --extra-resource=./r_codes --prune=true --out=release-builds-linux"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "path": "^0.12.7",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "electron": "11.5.0",
    "electron-packager": "15.2.0"
  }
}
